{
   "author": "Horacio Sanson",
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "114": "icons/pass114.png",
         "19": "icons/pass19.png",
         "38": "icons/pass38.png"
      },
      "default_popup": "popup.html",
      "default_title": "pass"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "description": "Chrome plugin for *pass* the standard unix password manager.",
   "homepage_url": "https://github.com/hsanson/chrome-pass",
   "icons": {
      "128": "icons/pass128.png",
      "16": "icons/pass16.png",
      "48": "icons/pass48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "50",
   "name": "chrome-pass (ZX2C4)",
   "permissions": [ "tabs", "nativeMessaging", "clipboardWrite" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.3.0"
}
